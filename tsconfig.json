{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "lib": ["esnext", "dom"],
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmit": true,
    "jsx": "react"
  }
}
